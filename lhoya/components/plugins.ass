.pagerbar {
  text-align: center;
  font-family: Arial;
  white-space: nowrap;
  font-size: 0;
}
.pagerbar span.ellipsis, .pagerbar a {
  height: 22px;
  font-size: 12px;
  line-height: 22px;
  background-color: #fff;
  display: inline-block;
  vertical-align: middle;
  text-decoration: none;
  text-align: center;
  overflow: hidden;
  border: 1px solid #e6e7f1;
  margin: 0 -1px;
  padding: 0 18px;
  color: #57666a;
  outline: none;
}
.pagerbar a:hover {
  color: #3c9bcb;
}
.pagerbar a.disabled, .pagerbar a.disabled:hover {
  color: #ccc;
  cursor: default;
}
.pagerbar a.last {
  border-radius: 3px 0 0 3px;
}
.pagerbar a.first {
  border-radius: 0 3px 3px 0;
}
.pagerbar a.current {
  background-color: #ebf4fa;
  cursor: default;
}
.pagerbar input.pn {
  margin-left: 30px;
  height: 22px;
  width: 39px;
  font-size: 12px;
  line-height: 22px;
  border-right: none;
  vertical-align: middle;
  border-radius: 3px 0 0 3px;
}
.pagerbar .go {
  border-radius: 0 3px 3px 0;
  font-size: 12px;
  height: 24px;
  line-height: 24px;
}
//pagerbar end